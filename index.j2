{% extends 'base.j2' %}

{% block title %}SCP Wiki Extracted Styles{% endblock %}

{% block body %}
  <h1 class="pages-header">Pages</h1>

  <ul class="pages-list">
    {% for page in pages %}
      <li>
        {% if page.error %}
          <span class="page-missing">{{ title }}</span> &mdash; <code>{{ page.error }}</code>
        {% else %}
          <a href="/{{ slug }}.html">{{ title }}</a>
        {% endif %}
      </li>
    {% endfor %}
  </ul>
{% endblock %}
